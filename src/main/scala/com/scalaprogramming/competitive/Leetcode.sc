/**
  * https://leetcode.com/problems/plus-one/
  *
  * Plus One
  */

def plusOne(digits: Array[Int]): Array[Int] = {

  if (digits.isEmpty) throw new IllegalArgumentException("Array is empty")

  digits(digits.size - 1) = digits.last + 1

  val out = digits.indices.foldRight(0: Int) {
    case (idx, carry) =>
      val x = digits(idx) + carry
      if (x > 9) {
        digits(idx) = x % 10
      } else {
        digits(idx) = x
      }
      x / 10 // Any carry to use for next iteration.
  }

  // If a carry is available at the end of iteration,
  // than the most Significant digit need to be created.
  // Hence, create a new array object.
  if (out > 0) Array(out) ++ digits else digits
}
plusOne(Array(1, 9, 9))
plusOne(Array(1, 9, 0))
plusOne(Array(9))

