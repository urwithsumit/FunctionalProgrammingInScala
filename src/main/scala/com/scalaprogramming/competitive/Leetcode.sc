def removeDuplicates(nums: Array[Int]): Int = {

  if (nums.isEmpty || nums.length < 2) nums.length
  else {
    val out = nums.indices.tail.foldLeft(0: Int, 0: Int) {
      case ((previous, count), current) => if (nums(current) == nums(previous)) {
        nums(current) = Int.MaxValue
        (previous, count + 1)
      } else (current, count)
    }

    nums.sortInPlace
    nums.length - out._2

    //Below is from Leet Code Discussions..
