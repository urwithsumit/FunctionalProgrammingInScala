/**
  * Find Third Man in an Array
  * //https://leetcode.com/explore/featured/card/fun-with-arrays/523/conclusion/3231/
  *
  * @param nums
  * @return
  */


def thirdMax(nums: Array[Int]): Int = {

  var h1, h2, h3 = Long.MinValue

  for (i <- nums) {
    if (i > h1) {
      h3 = h2
      h2 = h1
      h1 = i
    }
    else if (i > h2 && i != h1) {
      h3 = h2
      h2 = i
    }
    else if (i > h3 && i != h2 && i != h1) h3 = i
  }

  val ans = (h1 :: h2 :: h3 :: Nil).distinct.filter(_ > Long.MinValue)

  if (ans.size < 3) ans.max.toInt else ans.min.toInt

}

