def findDisappearedNumbers(nums: Array[Int]): List[Int] = {

  val sorted = nums.sortInPlace

  val out = (sorted :+ sorted.length + 1).foldLeft(List[Int](), 0: Int) {
    case ((acc, head), curr) =>
      if ((curr - head) > 1) {
        val result = for (i <- head + 1 until curr) yield i
        (acc :++ result, curr)
      } else (acc, curr)
  }

  out._1
}

