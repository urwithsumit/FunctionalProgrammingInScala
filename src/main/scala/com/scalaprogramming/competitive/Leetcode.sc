/**
  * https://leetcode.com/explore/featured/card/30-day-leetcoding-challenge/530/week-3/3304/
  * Runtime: 492 ms
  *
  * @param num
  * @param target
  * @return
  */
def search(num: Array[Int], target: Int): Int = {

  if (num.isEmpty) return -1

  /**
    * Returns the lower bound index at rotation point.
    * e.g. 4,5,0,1,2 will output 2 (index of 0)
    * O(log(n))
    *
    * @param lo
    * @param hi
    * @return
    */
  def rotateIdx(lo: Int, hi: Int): Int = {
    if (lo < hi) {
      if (math.abs(hi - lo) > 2) {
        val mid = (lo + hi) / 2

        if (num(mid) < num(mid - 1)) mid
        else if (mid + 1 < hi && num(mid) > num(mid + 1)) mid + 1
        else math.max(rotateIdx(lo, mid), rotateIdx(mid + 1, hi))

      } else {
        // can be  1, 2 or 3 length of array
        val mid = (lo + hi) / 2
        if (num(lo) > num(mid)) mid
        else if (num(mid) > num(hi)) hi
        else -1
      }
    } else lo
  }

  /**
    * Binary Search
    * O(log(n))
    *
    * @param lo
    * @param hi
    * @return
    */
  def _search(lo: Int, hi: Int): Int = {
    if (lo <= hi) {

      val mid: Int = (lo + hi) / 2

      if (num(mid) > target) _search(lo, mid - 1)
      else if (num(mid) < target) _search(mid + 1, hi)
      else mid

    } else -1
  }

  // calling the routines
  val idx = rotateIdx(0, num.length - 1)
  println(idx)

  if (idx > -1) {
    // Handle border scenario: When rotation point is the last index
    // in an array of size > 0
    // rotation index is not on array edge
    // {Left half Array} > idx < {right half Array}
    if (num(idx) == target) idx
    // This conditions prevents error in next 2 steps.
    //else if (idx == 0) _search(1, num.length - 1) // This will not happen, as I return the lower bound of rotation index
    else if (idx == num.length - 1) _search(0, num.length - 2)
    else math.max(_search(0, idx - 1), _search(idx, num.length - 1))

  } else _search(0, num.length - 1) // When no rotation index is available.
}

/**
