/**
  * Factor a number as per the given sorted list
  */

def findFactor(num: Int, fact: List[Int]): List[Int] = {
  fact.foldLeft(List[Int]()) {
    case (acc, c) => (acc ::: List.fill((num - acc.sum) / c)(c))
  }
}

findFactor(14, List(5, 2, 1))
findFactor(26, List(10, 5, 4, 3, 2, 1))

