//https://leetcode.com/problems/string-to-integer-atoi/
def myAtoi(str: String): Int = {
  import scala.util.matching.Regex
  import scala.collection.mutable.ListBuffer

  val pattern = "(\\d+)".r

  val strNumber = str.foldLeft(false: Boolean, ListBuffer[Int](), ' ': Char) {
    case ((flag, buff, sign), ch) => if (!flag) {
      ch match {
        case ' ' if buff.size == 0 && sign == ' ' => (flag, buff, sign)
        case '+' | '-' if buff.size == 0 && sign == ' ' => (flag, buff, ch)
        case pattern(ch) => (flag, buff.addOne(ch - '0'), sign)
        case _ => (true, buff, sign)
      }
    } else (flag, buff, sign)
  }

  val len = strNumber._2.length
  val sign = strNumber._3

  val ans = strNumber._2.foldLeft(0: Long, 1: Int) {
    case ((acc, idx), x) => if (x != 0) {
      val y = x * math.pow(10, len - idx)
      (acc + y.toLong, idx + 1)
    } else (acc, idx + 1)
  }

  val out = if (sign == '-') -ans._1 else ans._1

  if (ans._1.toString.length < 11) {
    if (out < Int.MinValue) Int.MinValue
    else if (out > Int.MaxValue) Int.MaxValue
    else out.toInt
  } else {
    if (sign == '-') Int.MinValue else Int.MaxValue
  }
}

