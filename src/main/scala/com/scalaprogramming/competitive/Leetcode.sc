  */

def divide(dividend: Int, divisor: Int): Int = {

  def _divide(dividend: Int, divisor: Int, acc: Int): Int = {
    (dividend, divisor) match {
      case (dividend, _) if (dividend == Int.MinValue) => _divide(Int.MaxValue, divisor, acc)
      case (_, 1) | (_, -1) => dividend * divisor
      case _ if dividend >= divisor => {
        val x = dividend - (divisor * 2)
        if (x == 0) acc + 2
        else if (x > 0)
          _divide(x, divisor, acc + 2)
        else {
          if (dividend - divisor == 0) acc + 1
          else
            _divide(dividend - divisor, divisor, acc + 1)
        }
      }
      case _ => acc
    }
  }

  val ans = _divide(math.abs(dividend), math.abs(divisor), 0)

  if (dividend < 0 && math.abs(ans) == Int.MinValue && divisor < 0) Int.MaxValue
  else if (((dividend < 0 && divisor > 0) || ((dividend > 0 && divisor < 0)))) {
    if (ans == Int.MaxValue) Int.MinValue else ans * -1
  } else ans
}

