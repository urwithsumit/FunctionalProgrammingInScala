/**
  * Write Insertion Sort Algorithm
  *
  * @param heights
  */
def insertionSort(heights: Array[Int]) = {

  var i, j = 0

  def swap(i: Int, j: Int): Unit = {
    val tmp = heights(i)
    heights(i) = heights(j)
    heights(j) = tmp
  }

  while (i < heights.length) {
    j = i
    while (j > 0 && heights(j) < heights(j - 1)) {
      swap(j, j - 1)
      j = j - 1
    }
    i += 1
  }
  println(heights.mkString(", "))
}

