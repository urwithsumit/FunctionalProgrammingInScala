/**
  * https://leetcode.com/explore/featured/card/fun-with-arrays/511/in-place-operations/3261/
  *
  * @param A
  * @return
  */
def sortedSquares(A: Array[Int]): Array[Int] = {

  val squared = A.map(c => c * c)
  var j = A.indexWhere(_ >= 0)

  if (j < 0) squared.reverse // i.e. Array has only -ve numbers
  else if (j > 0) { // i.e. array strictly has both +ve and -ve number.
    var i = j - 1
    var k = 0

    val merged = Array.ofDim[Int](A.length)

    while (i >= 0 && j < A.length) {
      if (squared(i) <= squared(j)) {
        merged(k) = squared(i)
        k += 1
        i -= 1
      } else {
        merged(k) = squared(j)
        k += 1
        j += 1
      }
    }

    while (i >= 0) {
      merged(k) = squared(i)
      i -= 1
      k += 1
    }

    while (j < A.length) {
      merged(k) = squared(j)
      k += 1
      j += 1
    }

    merged
  } else squared // Array has only +ve numbers.

}

