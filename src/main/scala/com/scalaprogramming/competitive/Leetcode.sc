/**
  * Longest Substring without repeating Character
  * This Solution was not accepted.
  */
def _lengthOfLongestSubstring(str: String): Int = {

  val s = str.toCharArray
  var count = 0
  for {
    i <- s.length to 1 by -1 // Go in Reverse order
    if (i > count) // only when current index is greater than Count. No point iterating for lower index.
  } yield {
    s.sliding(i).exists(c => c.distinct.size == c.size) match {
      case true => count = i
      case _ =>
    }
  }

  count

}

/** *
  * This Solution was accepted...
  *
  * @param str
  * @return
  */
def lengthOfLongestSubstring(str: String): Int = {
  val result = {
    str.foldLeft(Set[Int](), new StringBuilder()) {
      case ((acc, sb), x) =>
        val idx = sb.indexOf(x)
        if (idx > -1) {
          (acc + sb.slice(0, idx + 1).size,
            sb.slice(idx + 1, sb.size).append(x))
        } else {
          val z = sb.append(x)
          (acc + z.size, z)
        }
    }
  }._1

  if (result.nonEmpty) result.max else 0
}

