/**
* https://leetcode.com/problems/remove-element/
*
* With additional Queue data Structure
*
*/
def removeElement1(nums: Array[Int], K: Int): Int = {
  val stack = scala.collection.mutable.Queue[Int]()
  for (i <- nums.length - 1 to 0 by -1) {
    if (nums(i) == K) {
      if (!stack.isEmpty) {
        nums(i) = nums(stack.front)
        nums(stack.dequeue()) = Int.MinValue
        stack.enqueue(i)
      } else nums(i) = Int.MinValue
    } else {
      stack.enqueue(i)
    }
  }
  if (nums.nonEmpty) {
    nums.length - nums.count(_ == Int.MinValue)
  } else 0
}

/**
* https://leetcode.com/problems/remove-element/
*
* with O(1) memory for pointer.
*/
def removeElement(nums: Array[Int], K: Int): Int = {
  var j = nums.length - 1
  for (i <- nums.length - 1 to 0 by -1) {
    if (nums(i) == K) {
      if (i <= j) {
        nums(i) = nums(j)
        nums(j) = K
        j -= 1
      }
    }
  }

  if (nums.nonEmpty) j + 1 else 0
}

