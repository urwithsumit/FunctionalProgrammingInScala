def search2(num: Array[Int], target: Int): Int = {

  var lo = 0
  var hi = num.length - 1
  var idx = -1

  while (lo <= hi && idx == -1) {
    if (num(lo) == target) idx = lo
    else if (num(hi) == target) idx = hi
    else {
      lo += 1
      hi -= 1
    }
  }
  idx
}

